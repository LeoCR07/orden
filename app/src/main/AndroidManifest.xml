<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.orden">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Orden">
        <activity android:name=".Configuracion.NumeroMesaActivity"></activity>
        <activity android:name=".Comanda.MainComandaActivity" />
        <activity android:name=".Caja.CajaActivity" />
        <activity android:name=".Caja.FacturaActivity" />
        <activity
            android:name=".Actividad.MesasActivity"
            android:parentActivityName=".Principal.MainActivity" />
        <activity
            android:name=".Configuracion.ConfiguracionActivity"
            android:parentActivityName=".Principal.MainActivity" />
        <activity
            android:name=".Comanda.ComandaA1Activity"
            android:parentActivityName=".Principal.MainActivity" />
        <activity android:name=".Login.LoginActivity" />
        <activity android:name=".Splash.SplashActivity" />
        <activity android:name=".Principal.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>